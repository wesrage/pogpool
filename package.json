{
  "name": "pogpool",
  "version": "3.0.0",
  "description": "PuckOverGlass Stanley Cup Playoff Pool",
  "license": "ISC",
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev\" \"npm run dev-api\"",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "dev-api": "better-npm-run dev-api",
    "prod": "better-npm-run prod-api",
    "test": "jest --watch"
  },
  "betterScripts": {
    "dev": {
      "command": "node index",
      "env": {
        "API_HOST": "localhost",
        "API_PORT": 3030,
        "WEB_PORT": 3000
      }
    },
    "dash": {
      "command": "webpack-dashboard -- node index",
      "env": {
        "API_HOST": "localhost",
        "API_PORT": 3030,
        "WEB_PORT": 3000
      }
    },
    "dev-api": {
      "command": "node api",
      "env": {
        "API_HOST": "localhost",
        "API_PORT": 3030,
        "WEB_HOST": "http://localhost:3000"
      }
    },
    "prod-api": {
      "command": "node api",
      "env": {
        "API_HOST": "localhost",
        "API_PORT": 80,
        "WEB_HOST": "http://w.esley.ca",
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack.config.prod.babel.js",
      "env": {
        "API_HOST": "api.pog.esley.ca",
        "API_PORT": "80"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wesrage/pogpool.git"
  },
  "moduleRoots": [
    "api",
    "src"
  ],
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-jest": "^15.0.0",
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^15.1.1",
    "react-test-renderer": "^15.4.2",
    "redux-devtools-extension": "^2.13.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "better-npm-run": "^0.0.11",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "concurrently": "^2.2.0",
    "cors": "^2.8.3",
    "css-loader": "^0.26.0",
    "eslint-plugin-babel": "^3.3.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "http-proxy": "^1.16.2",
    "lodash.pick": "^4.4.0",
    "moment": "^2.18.1",
    "monk": "^4.0.0",
    "obex": "^1.1.0",
    "piping": "^1.0.0-rc.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.3",
    "react-icons": "^2.2.3",
    "react-media": "^1.5.1",
    "react-notification-system": "^0.2.13",
    "react-redux": "^4.4.6",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1",
    "styled-components": "^1.4.4",
    "uuid": "^3.0.1",
    "webpack": "^2.3.3"
  }
}
