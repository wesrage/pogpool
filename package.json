{
  "name": "pogpool",
  "version": "3.2.0",
  "description": "PuckOverGlass Stanley Cup Playoff Pool",
  "license": "UNLICENSED",
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev\" \"npm run dev-api\"",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "dev-api": "better-npm-run dev-api",
    "test": "jest",
    "prod": "better-npm-run prod-api",
    "updater": "node scraper"
  },
  "betterScripts": {
    "dev": {
      "command": "node index",
      "env": {
        "API_HOST": "localhost",
        "API_PORT": 3030,
        "HTTP_PORT": 3000
      }
    },
    "dev-api": {
      "command": "node api",
      "env": {
        "API_HOST": "localhost",
        "API_PORT": 3030
      }
    },
    "prod-api": {
      "command": "node api",
      "env": {
        "API_HOST": "localhost",
        "API_PORT": 8080,
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details",
      "env": {
        "API_HOST": "api.pog.esley.ca",
        "API_PORT": 8080,
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wesrage/pogpool.git"
  },
  "moduleRoots": [
    "api",
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.9",
    "better-npm-run": "^0.1.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.5.0",
    "piping": "^1.0.0-rc.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3": "^3.5.17",
    "emotion": "^10.0.9",
    "express": "^4.16.4",
    "history": "^4.9.0",
    "http-proxy": "^1.17.0",
    "immer": "^2.1.5",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "monk": "^6.0.6",
    "node-sass": "^4.11.0",
    "obex": "^1.2.1",
    "pretty-error": "^2.1.1",
    "qhistory": "^1.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.5.0",
    "react-media": "^1.9.2",
    "react-notification-system": "^0.2.17",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "styled-components": "^4.2.0"
  }
}
